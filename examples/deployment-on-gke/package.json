{
  "name": "deployment-on-gke",
  "version": "1.0.0",
  "description": "This is an example deployment repo as described in [this document](../docs/deployment-on-gke.md). The code here is what you'd expect to see in the deployment repository for a wordpress project.",
  "main": "index.js",
  "bin": {
    "comfy": "scripts/comfy.js"
  },
  "scripts": {
    "test": "node ./scripts/comfy test",
    "clone": "node ./scripts/comfy clone",
    "build": "node ./scripts/comfy build",
    "push": "node ./scripts/comfy push",
    "tag": "node ./scripts/comfy tag",
    "deploy": "node ./scripts/comfy deploy",
    "deploy:prod": "node ./scripts/comfy deploy --prod",
    "delete": "node ./scripts/comfy delete",
    "rollback": "node ./scripts/comfy rollback"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bluebird": "^3.4.6",
    "child-process-promise": "^2.1.3",
    "commander": "^2.9.0",
    "fs-extra-promise": "^0.4.1"
  }
}
